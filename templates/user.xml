<templateSet group="user">
  <template name="pvm" value="public void $NAME$(){&#10;$END$&#10;}" description="public void method" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="pf" value="private final $NAME$" description="private final" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="posting" value="---&#10;layout: post&#10;category:&#10;    - category1&#10;    - category2&#10;date: $DATE$&#10;title: title&#10;description: &gt;&#10;    desc&#10;image: /assets/img/&#10;accent_image:&#10;    background: url('/assets/img/') center/cover&#10;related_posts:&#10;    - path&#10;    - path&#10;---&#10;&#10;* toc&#10;{:toc}&#10;&#10;&amp;nbsp;  &#10;&#10;$END$" description="" toReformat="false" toShortenFQNames="false">
    <variable name="DATE" expression="date(&quot;Y-MM-d HH:mm&quot;)" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ANY_OPENAPI_JSON_FILE" value="false" />
      <option name="ANY_OPENAPI_YAML_FILE" value="false" />
      <option name="ASPECTJ" value="false" />
      <option name="CSS" value="false" />
      <option name="CUCUMBER_FEATURE_FILE" value="false" />
